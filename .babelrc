{
  "presets": ["es2015", "react", "stage-0"],
  "env": {
    "development": {
      "plugins": ["react-hot-loader/babel"]
    },
    "commonjs": {
      "plugins": ["transform-es2015-modules-commonjs", { "loose": true }]
    },
    "es": {
      "presets": ["es2015-loose-native-modules"]
    }
  }
}
